	<ItemGroup>
		<Compile Remove="Output\**" />
		<EmbeddedResource Remove="Output\**" />
		<None Remove="Output\**" />
		<Page Remove="Output\**" />
	</ItemGroup>
	<ItemGroup>
		<None Include="plugin.json">
