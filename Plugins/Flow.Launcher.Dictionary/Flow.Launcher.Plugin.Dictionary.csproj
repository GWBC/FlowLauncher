	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
		<OutputPath>..\..\Output\Release\Plugins\Flow.Launcher.Plugin.Dictionary\</OutputPath>
	</PropertyGroup>
